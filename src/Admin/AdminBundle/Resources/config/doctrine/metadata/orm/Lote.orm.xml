<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lote" table="lote">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cantidad" type="integer" column="cantidad" nullable="false"/>
    <field name="idDestino" type="integer" column="id_destino" nullable="false"/>
    <field name="idUsuAlta" type="integer" column="id_usu_alta" nullable="false"/>
    <field name="codBarra" type="string" column="cod_barra" length="50" nullable="false"/>
    <field name="fechaReg" type="datetime" column="fecha_reg" nullable="false"/>
  </entity>
</doctrine-mapping>
